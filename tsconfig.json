{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["es2018"],
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "dist",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "es2015"
  },
  "include": ["src"]
}
